record(ai,"$(P)FREQ"){
    field(DTYP,"Soft Channel")
    field(VAL,0)
    field(PREC, "3")
    field(EGU, "Hz")
    field(LOPR, 0)
	field(INP, "$(P)FREQ:SP")
    field(SCAN, ".1 second")
}
